[ignore]
.*/build/.*

[include]
./node_modules/@polkadot/dev/flow-typed

[libs]

[options]
esproposal.decorators=ignore
module.ignore_non_literal_requires=true
unsafe.enable_getters_and_setters=true
module.name_mapper='^@polkadot/client-p2p\(.*\)$' -> '<PROJECT_ROOT>/../client-p2p/src\1'
module.name_mapper='^@polkadot/client-rpc\(.*\)$' -> '<PROJECT_ROOT>/../client-rpc/src\1'
